{
  "name": "marksense",
  "authorizedClientUids": [
    "marksense"
  ],
  "languages": {
    "en": {
      "prediction": {
        "enabled": true
      },
      "autocorrection": {
        "enabled": true
      },
      "grammar": {
        "enabled": false
      },
      "language_detection": {
        "enabled": true
      }
    },
    "de": {
      "prediction": {
        "enabled": true
      },
      "autocorrection": {
        "enabled": true
      },
      "grammar": {
        "enabled": false
      },
      "language_detection": {
        "enabled": true
      }
    }
  },
  "global_library_settings": {
    "wordlists": {
      "company_specific_wordlists": {
        "en": [
          "l=en-gb_k=standard",
          "l=en_k=curly-apostrophe",
          "l=en_k=acronyms",
          "l=en_k=foreign-words",
          "l=en_k=formal-informal",
          "l=all_k=apps",
          "l=all_k=brands",
          "l=all_k=companies",
          "l=all_k=consumer-hardware-companies",
          "l=all_k=games",
          "l=all_k=hardware",
          "l=all_k=online-tools",
          "l=all_k=software",
          "l=all_k=technologies",
          "l=all_k=technology-terms"
        ]
      }
    }
  },
  "correction_config": {
    "server": true,
    "base_model_name": "v2.0_API",
    "ngram_version": "v2.15",
    "don't_predict_words": {
      "en": [
        "basse",
        "everyones",
        "handel",
        "heres",
        "hows",
        "oughtve",
        "thatre",
        "thats",
        "theres",
        "thesere",
        "theseve",
        "thosere",
        "thoseve",
        "ting",
        "tings",
        "whats",
        "whens",
        "wheres",
        "whys",
        "isn"
      ]
    },
    "don't_correct_away_from_words": {
      "en": [
        "hitlist"
      ]
    },
    "edit_distance_weights_overwrite": {
      "deletion_weight": 1,
      "insertion_weight": 1,
      "substitution_weight": 1,
      "transposition_weight": 0.1,
      "lower_insertion_weight": 0.35,
      "lower_transposition_weight": 1,
      "increased_deletion_weight": 1.5,
      "increased_substitution_weight": 1.5
    },
    "hardcoded_corrections": {
      "en": {
        "alot": "a lot",
        "Alot": "A lot",
        "visability": "visibility",
        "Visability": "Visibility",
        "brough": "brought",
        "youd": "you'd",
        "Youd": "You'd",
        "seperate": "separate",
        "Seperate": "Separate",
        "guage": "gauge",
        "Gauge": "Gauge",
        "accidently": "accidentally",
        "Accidently": "Accidentally",
        "touchbase": "touch base",
        "Touchbase": "Touch base",
        "bandwith": "bandwidth",
        "Bandwith": "Bandwidth",
        "commiting": "committing",
        "Commiting": "Committing",
        "severly": "severely",
        "Severly": "Severely",
        "burried": "buried",
        "Burried": "Buried",
        "transfered": "transferred",
        "Transferred": "Transferred",
        "calender": "calendar",
        "Calender": "Calendar",
        "handel": "handle",
        "tings": "things",
        "caht": "chat",
        "AJay": "AJay",
        "Elad": "Elad",
        "teir": "tier",
        "Teir": "Tier"
      }
    },
    "settings": {
      "allow_correction_compoundNouns": false,
      "auto_caps": true,
      "capitalization_implementation": "dictionary_counts",
      "capitalize_first_word": true,
      "compound_noun_length_threshold": 3,
      "compound_noun_titlecase_threshold": 100,
      "correct_accidental_capitalization": true,
      "correct_first_line_names": true,
      "correct_single_letter_words": false,
      "correct_upper_case_words": true,
      "default_number_of_spacesplits": 1,
      "default_previous_correction_best_score": 2,
      "ignore_scores_less_than": 0.0001,
      "inference_model": "tflite",
      "keep_current_word_bias": 0,
      "language_model_K": 0.005,
      "max_edit_distance": 3,
      "max_edit_distance_second_split": 2,
      "max_suggestion_length_difference": 2,
      "min_no_unigram_counts": 4,
      "prefix_length": 6,
      "single_letters_to_allow_autocorrect": {},
      "space_neighbours_scaling": 1.3,
      "spacesplit_on_each_key": true,
      "spacesplit_on_space_neighbours": false,
      "split_prob_reduction": 0.01,
      "transpose_around_space": true,
      "uppercase_bias": {
        "strong": 0.3,
        "weak": 0.1
      },
      "verbosity_nospacesplit": 2,
      "verbosity_spacesplit": 2
    },
    "properties": {
      "capitalize_after_punctuation": [
        ".",
        "?",
        "!",
        "\n"
      ],
      "ignore_capitalization_after_salutation_languages": [
        "de",
        "de-ch"
      ],
      "sos_token": "$NA$",
      "unknown_token": "$UNK$"
    },
    "neural_properties": {
      "include_neural_network": false
    },
    "abbreviations": {
      "en": [
        "e.g.",
        "c.f.",
        "i.e.",
        "etc.",
        "v.",
        "viz.",
        "sc.",
        "al.",
        "ca.",
        "approx.",
        "incl.",
        "mgmt.",
        "inc."
      ]
    },
    "blocklist": {
      "en": [],
      "de-ch": []
    },
    "blocklist_wordlist_names": {
      "de": [
        "l=de_v=4.0_k=blocklistfromlistofwords"
      ],
      "en": [
        "l=en_v=4.0_k=blocklistfromlistofwords"
      ]
    },
    "casing_overwrite": {
      "de-ch": {}
    }
  },
  "special_configs": []
}
